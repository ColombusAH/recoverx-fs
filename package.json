{
  "name": "react-redux-template",
  "version": "2.3.0",
  "type": "module",
  "description": "Starter kit for modern web applications",
  "homepage": "https://react-pwa.surenatoyan.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/suren-atoyan/react-pwa.git"
  },
  "bugs": {
    "url": "https://github.com/suren-atoyan/react-pwa/issues",
    "email": "contact@surenatoyan.com"
  },
  "license": "MIT",
  "scripts": {
    "tauri": "tauri",
    "dev": "tsc && vite --mode=development",
    "dev-node": "tsx watch src/server",
    "start": "node dist/server/",
    "build": "vite --mode=production build && tsc -p tsconfig.server.json",
    "build-be": " tsc -p tsconfig.server.json",
    "preview": "vite --mode=development preview",
    "https-preview": "serve dist",
    "prepare": "husky install && cp -n env/.shared .env &",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint:check": "eslint --max-warnings=0 \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "ts:check": "tsc --noEmit",
    "test:unit": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/lab": "5.0.0-alpha.156",
    "@mui/material": "^5.15.0",
    "@mui/system": "^5.15.0",
    "@mui/utils": "^5.15.0",
    "@reduxjs/toolkit": "^2.2.1",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "express": "^4.19.2",
    "is-mobile": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^2.0.3",
    "react-hotkeys-hook": "^4.4.1",
    "react-intl": "^6.6.2",
    "react-query": "^3.39.3",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.0",
    "remult": "^0.26.13",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@storybook/builder-vite": "^7.6.17",
    "@tauri-apps/cli": "^1.5.10",
    "@testing-library/react": "^14.2.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@types/react-redux": "^7.1.33",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.3.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "https-localhost": "^4.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "tsx": "^4.11.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.9",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.1.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --max-warnings=0"
    ]
  },
  "author": {
    "name": "Suren Atoyan",
    "email": "contact@surenatoyan.com",
    "url": "http://surenatoyan.com/"
  },
  "keywords": [
    "react",
    "pwa",
    "starter-kit",
    "vite"
  ]
}
